<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dao.QichexinxiDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.entity.QichexinxiEntity" id="qichexinxiMap">
        <result property="chepaihao" column="chepaihao"/>
        <result property="qichexinghao" column="qichexinghao"/>
        <result property="qichefenlei" column="qichefenlei"/>
        <result property="pinpai" column="pinpai"/>
        <result property="yanse" column="yanse"/>
        <result property="huandangfangshi" column="huandangfangshi"/>
        <result property="fengmian" column="fengmian"/>
        <result property="zhuangtai" column="zhuangtai"/>
        <result property="zujin" column="zujin"/>
        <result property="yajin" column="yajin"/>
        <result property="cheliangpailiang" column="cheliangpailiang"/>
        <result property="youhao" column="youhao"/>
        <result property="zaizhong" column="zaizhong"/>
        <result property="zulinxuzhi" column="zulinxuzhi"/>
        <result property="guihaibiaozhun" column="guihaibiaozhun"/>
        <result property="dengjiriqi" column="dengjiriqi"/>
        <result property="cheliangxiangqing" column="cheliangxiangqing"/>
        <result property="clicktime" column="clicktime"/>
        <result property="clicknum" column="clicknum"/>
        <result property="storeupnum" column="storeupnum"/>
    </resultMap>

	<select id="selectListVO"
		resultType="com.entity.vo.QichexinxiVO" >
		SELECT * FROM qichexinxi  qichexinxi         
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectVO"
		resultType="com.entity.vo.QichexinxiVO" >
		SELECT  qichexinxi.* FROM qichexinxi  qichexinxi 	
 		<where> 1=1 ${ew.sqlSegment}</where>
	</select>

    <select id="selectListView"
		resultType="com.entity.view.QichexinxiView" >

		SELECT  qichexinxi.* FROM qichexinxi  qichexinxi 	        
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectView"
		resultType="com.entity.view.QichexinxiView" >
		SELECT * FROM qichexinxi  qichexinxi <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	

    <select id="selectValue" resultType="map" >
        SELECT ${params.xColumn}, ROUND(sum(${params.yColumn}),1) total FROM qichexinxi
        <where> 1=1 ${ew.sqlSegment}</where>
        group by ${params.xColumn}
    </select>

    <select id="selectTimeStatValue" resultType="map" >
        <if test = 'params.timeStatType == "日"'>
                SELECT DATE_FORMAT(${params.xColumn},'%Y-%m-%d') ${params.xColumn}, sum(${params.yColumn}) total FROM qichexinxi
                <where> 1=1 ${ew.sqlSegment}</where>
                group by DATE_FORMAT(${params.xColumn},'%Y-%m-%d')
        </if>
        <if test = 'params.timeStatType == "月"'>
                SELECT DATE_FORMAT(${params.xColumn},'%Y-%m') ${params.xColumn}, sum(${params.yColumn}) total FROM qichexinxi
                <where> 1=1 ${ew.sqlSegment}</where>
                group by DATE_FORMAT(${params.xColumn},'%Y-%m')
        </if>
        <if test = 'params.timeStatType == "年"'>
                SELECT DATE_FORMAT(${params.xColumn},'%Y') ${params.xColumn}, sum(${params.yColumn}) total FROM qichexinxi
                <where> 1=1 ${ew.sqlSegment}</where>
                group by DATE_FORMAT(${params.xColumn},'%Y')
        </if>
    </select>

    <select id="selectGroup" resultType="map" >
        SELECT ${params.column} , count(1) total FROM qichexinxi
        <where> 1=1 ${ew.sqlSegment}</where>
        group by ${params.column}
    </select>




</mapper>
